#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/amine/frontend_projects/send/sps/node_modules/.pnpm/esbuild@0.14.49/node_modules/esbuild/bin/node_modules:/home/amine/frontend_projects/send/sps/node_modules/.pnpm/esbuild@0.14.49/node_modules/esbuild/node_modules:/home/amine/frontend_projects/send/sps/node_modules/.pnpm/esbuild@0.14.49/node_modules:/home/amine/frontend_projects/send/sps/node_modules/.pnpm/node_modules:/home/amine/frontend_projects/send/sps/node_modules:/home/amine/frontend_projects/send/node_modules:/home/amine/frontend_projects/node_modules:/home/amine/node_modules:/home/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/amine/frontend_projects/send/sps/node_modules/.pnpm/esbuild@0.14.49/node_modules/esbuild/bin/node_modules:/home/amine/frontend_projects/send/sps/node_modules/.pnpm/esbuild@0.14.49/node_modules/esbuild/node_modules:/home/amine/frontend_projects/send/sps/node_modules/.pnpm/esbuild@0.14.49/node_modules:/home/amine/frontend_projects/send/sps/node_modules/.pnpm/node_modules:/home/amine/frontend_projects/send/sps/node_modules:/home/amine/frontend_projects/send/node_modules:/home/amine/frontend_projects/node_modules:/home/amine/node_modules:/home/node_modules:/node_modules"
fi
"$basedir/../../bin/esbuild"   "$@"
exit $?
